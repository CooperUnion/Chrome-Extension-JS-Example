{
  "name": "A demo",
  "description": "An example chrome extension that injects buttons using Javascript",
  "version": "0.1.1",
  "content_scripts": [
    {
      "matches": ["https://*/*"],
      "css": ["style.css"],
      "js": ["script.js"]
    }
